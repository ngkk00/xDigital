{
  "name": "backend_node",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "db:seed": "ts-node src/seed.ts",
    "typeorm": "tsx ./node_modules/typeorm/cli.js -d ./src/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "swagger:generate": "tsoa spec-and-routes",
    "swagger:serve": "npm run swagger:generate && npm run dev",
    "tsoa:routes": "tsoa routes",
    "tsoa:spec": "tsoa spec",
    "tsoa:build": "npm run tsoa:routes && npm run tsoa:spec && tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.3",
    "@types/pg": "^8.15.5",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "pg": "^8.16.3",
    "express": "^5.1.0",
    "swagger-ui-express": "^5.0.1",
    "process": "^0.11.10",
    "reflect-metadata": "^0.2.2",
    "ts-node-dev": "^2.0.0",
    "tsoa": "^6.6.0",
    "typeorm": "^0.3.26",
    "uuid": "^13.0.0"
  }
}